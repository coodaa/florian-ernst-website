{
  "name": "yarn-build",
  "version": "0.9.0",
  "main": "./bin/@yarnpkg/plugin.build.js",
  "homepage": "https://yarn.build",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/yup": "0.26.12",
    "@yarnpkg/builder": "^2.0.0-rc.19",
    "@yarnpkg/cli": "^2.0.0-rc.30",
    "@yarnpkg/core": "^2.0.0-rc.24",
    "@yarnpkg/fslib": "^2.0.0-rc.17",
    "@yarnpkg/libzip": "^2.0.0-rc.10",
    "@yarnpkg/plugin-pack": "2.0.0-rc.18",
    "jest": "^25.3.0",
    "jest-pnp-resolver": "^1.2.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@yarnpkg/fslib": "^2.0.0-rc.17",
    "@yarnpkg/libzip": "^2.0.0-rc.10",
    "@yarnpkg/parsers": "^2.0.0-rc.10",
    "@yarnpkg/plugin-pack": "2.0.0-rc.18",
    "@yarnpkg/plugin-pnp": "^2.0.0-rc.19",
    "@yarnpkg/pnp": "^2.0.0-rc.18",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "await-semaphore": "0.1.3",
    "clipanion": "^2.1.5",
    "date-fns": "^2.12.0",
    "p-limit": "^2.3.0",
    "p-queue": "^6.3.0",
    "strip-ansi": "^6.0.0"
  },
  "scripts": {
    "build:plugin-stage": "builder build plugin",
    "update-local": "yarn build:plugin-stage && rsync -a --delete bundles/ bin/",
    "update-workspace": "mkdir -p ../../../.yarn/plugins/@yarnpkg && cp ./bundles/@yarnpkg/plugin-build.js ../../../.yarn/plugins/plugin-build.js",
    "update": "yarn update-local && yarn update-workspace",
    "build": "update",
    "postpack": "rm -rf lib",
    "clean": "rimraf build bin",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/KablamoOSS/yarn-build.git"
  },
  "publishConfig": {
    "main": "./bin/@yarnpkg/plugin.build.js",
    "typings": "./lib/index.d.ts"
  },
  "files": [
    "/lib/**/*"
  ]
}